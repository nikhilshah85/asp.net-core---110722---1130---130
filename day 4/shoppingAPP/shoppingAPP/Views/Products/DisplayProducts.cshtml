


<h1> List of Products </h1>


<table class="table">

    @foreach (var item in ViewBag.products)
    {
        <tr>
            <td> @item.pId </td>
            <td> @item.pName </td>
            <td> @item.pCategory </td>
            <td> @item.pPrice </td>
            <td> @item.pIsInStock </td>
            <td> <button class="btn btn-primary"> Add To Cart </button> </td>
        </tr>
    }
</table>
